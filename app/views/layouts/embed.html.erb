<!doctype html>      
<html lang="en">
<head>
  <link rel="shortcut icon" href="/favicon.ico" />
  <meta name="description" content="<%= @form.nil? ?  "" : @form.description %>" />
  <meta name='viewport' content='width=device-width' />
  <meta charset="utf-8" />
  <meta name="csrf-token" content="<%= form_authenticity_token %>" />
  <meta name="csrf-param" content="authenticity_token" />
  <!--[if IE]>
   <script src="/javascripts/html5.js"></script>
  <![endif]-->                   
  <%= stylesheet_link_tag "public",'generated_css/common', :cache => 'embed.cache' %> 
  <%= javascript_include_tag 'jquery', 'jquery-ui', 'jrails', 'rails','application', :cache => 'app.cache' %> 
  <%= yield :headers %>                                                   
  <title><%= @form.nil? ? (@page_title||'') : @form.title %></title> 
  <style type="text/css" media="screen">
    body{
      background:#fff;
    }
  </style>
</head>

<body>
  <div id="saving" style="display:none;">Saving...</div>    
  <%= yield %>
</body>
</html>
